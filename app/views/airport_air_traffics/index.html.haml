- model_class = AirportAirTraffic
.page-header
  %h1=t '.title', :default => model_class.model_name.human.pluralize.titleize

%h1 Listing airport_air_traffics

%table.table.table-striped.table-hover.table-condensed
  %thead
    %tr
      %th Name
      %th Klass
      %th
      %th
      %th

  %tbody
    - @airport_air_traffics.each do |airport_air_traffic|
      %tr
        %td= airport_air_traffic.name
        %td= airport_air_traffic.klass
        %td= link_to 'Show', airport_air_traffic
        %td= link_to 'Edit', edit_airport_air_traffic_path(airport_air_traffic)
        %td= link_to 'Destroy', airport_air_traffic, :method => :delete, :data => { :confirm => 'Are you sure?' }

= link_to t('.new', :default => t("helpers.links.new")), new_airport_air_traffic_path, :class => 'btn btn-primary'


<!DOCTYPE html>
<html>
  <%= render "layouts/head" %>
  <body>
    <div id="all">
      <p><%= flash[:alert]%></p>
      <p><%= flash[:notice]%></p>
      <% if user_signed_in? %>
        <%=render "layouts/header"%>
        <%=render "layouts/middle"%>
      <%else%>
        <%= yield %>
      <%end%>
    </div>
    <%=render "layouts/footer"%>
  </body>
</html>

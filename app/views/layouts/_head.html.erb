  <title><%= title %></title>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <% if user_signed_in? %>
    <%= stylesheet_link_tag    "desktop/style/application", :media => "all" %>
    <%= stylesheet_link_tag    "jquery-ui/#{current_user.user_theme.theme}", :media => "all" %>
    <%= stylesheet_link_tag    "desktop/color/#{current_user.user_theme.theme}/styles", :media => "all" %>
    <%= stylesheet_link_tag    "desktop/color/#{current_user.user_theme.theme}/paginate_page", :media => "all" %>
    <%= stylesheet_link_tag    "desktop/color/#{current_user.user_theme.theme}/login", :media => "all" %>
  <%else%>
    <%= stylesheet_link_tag    "untheme/application", :media => "all" %>
  <%end%>
  <%= javascript_include_tag "desktop/application" %>
  <script type="text/javascript">
    hs.graphicsDir = '/images/highslide/';
    hs.outlineType = 'rounded-black';
    hs.wrapperClassName = 'draggable-header';
    hs.dragSensitivity = 20;
  </script>
  <%= csrf_meta_tags %>

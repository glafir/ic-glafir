- model_class = StationPriority
.page-header
  %h1=t '.title', :default => model_class.model_name.human.pluralize.titleize

%h1 Listing station_priorities

%table.table.table-striped.table-hover.table-condensed
  %thead
    %tr
      %th Station
      %th Rw route
      %th Priority
      %th
      %th
      %th

  %tbody
    - @station_priorities.each do |station_priority|
      %tr
        %td= station_priority.station_id
        %td= station_priority.rw_route_id
        %td= station_priority.priority
        %td= link_to 'Show', station_priority
        %td= link_to 'Edit', edit_station_priority_path(station_priority)
        %td= link_to 'Destroy', station_priority, :method => :delete, :data => { :confirm => 'Are you sure?' }

= link_to t('.new', :default => t("helpers.links.new")), new_station_priority_path, :class => 'btn btn-primary'

